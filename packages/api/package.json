{
  "name": "@studiodrishti/recipe-lab-api",
  "description": "Play with your food.",
  "version": "0.0.1",
  "license": "AGPL-3.0-or-later",
  "repository": "https://github.com/studiodrishti/recipe-lab",
  "main": "dist/app.js",
  "targets": {
    "main": {
      "context": "node",
      "includeNodeModules": true
    }
  },
  "scripts": {
    "build": "yarn clean && yarn prisma:generate && yarn compile",
    "clean": "rm -rf dist generated",
    "compile": "parcel build app.js",
    "start": "node server.js",
    "deploy": "serverless deploy",
    "prisma:save": "prisma migrate save --experimental",
    "prisma:up": "prisma migrate up --experimental",
    "prisma:generate": "prisma generate",
    "prisma:seed": "node seed.js",
    "prisma:studio": "prisma studio --experimental",
    "bootstrap": "yarn prisma:up && yarn prisma:generate && yarn prisma:seed"
  },
  "dependencies": {
    "@graphql-tools/schema": "6.1.0",
    "apollo-server-express": "2.17.0",
    "bcryptjs": "^2.4.3",
    "cuid": "2.1.8",
    "express": "4.17.1",
    "graphql": "15.3.0",
    "graphql-middleware": "4.0.2",
    "graphql-shield": "7.3.5",
    "jsonwebtoken": "8.5.1",
    "mathjs": "^7.2.0",
    "morgan": "1.10.0",
    "speakingurl": "14.0.1",
    "validator": "13.1.1"
  },
  "engines": {
    "node": ">=12.*"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@prisma/cli": "2.5.1",
    "@prisma/client": "2.5.1",
    "parcel": "next",
    "serverless": "^1.80.0"
  }
}
