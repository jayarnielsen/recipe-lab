{
  "name": "schooled-lunch",
  "description": "Play with your food.",
  "version": "0.0.1",
  "main": "server/index.js",
  "license": "GPL-3.0-or-later",
  "repository": "https://github.com/jayarnielsen/schooled-lunch",
  "bin": {
    "www": "./bin/www.js"
  },
  "scripts": {
    "start": "node server",
    "dev-docker": "../node_modules/nodemon/bin/nodemon.js --debug=5858",
    "dev-host": "nodemon --debug=5858",
    "start-watch": "nodemon server --inspect=0.0.0.0:9229",
    "start-wait-debuger": "nodemon server --inspect-brk=0.0.0.0:9229",
    "test": "cross-env NODE_ENV=test PORT=8081 node --inspect=0.0.0.0:9230 /opt/node_modules/.bin/jest",
    "test-watch": "cross-env NODE_ENV=test PORT=8081 node --inspect=0.0.0.0:9230 /opt/node_modules/.bin/jest --runInBand --no-cache --watch",
    "test-wait-debuger": "cross-env NODE_ENV=test PORT=8081 node --inspect-brk=0.0.0.0:9230 /opt/node_modules/.bin/jest",
    "test-cicd": "jest",
    "lint-css": "stylelint \"client/**/*.css\"",
    "lint-js": "eslint \"**/*.{js,jsx}\"",
    "prettier": "prettier --write \"**/*.{js,jsx,json,yml,xml,md,css}\"",
    "build": "next build",
    "seed": "node ./server/seed"
  },
  "dependencies": {
    "@zeit/next-css": "1.0.1",
    "classnames": "2.2.6",
    "connect-mongo": "2.0.3",
    "cors": "2.8.5",
    "diff-match-patch": "1.0.4",
    "dotenv": "6.2.0",
    "express": "4.16.4",
    "express-session": "1.15.6",
    "filepond": "4.2.0",
    "filepond-plugin-image-crop": "2.0.1",
    "filepond-plugin-image-resize": "2.0.2",
    "filepond-plugin-image-transform": "3.2.2",
    "isomorphic-unfetch": "3.0.0",
    "lusca": "1.6.1",
    "mongoose": "5.4.15",
    "morgan": "1.9.1",
    "multer": "1.4.1",
    "next": "8.0.4-canary.2",
    "next-auth": "1.12.1",
    "nodemailer": "5.1.1",
    "nodemailer-direct-transport": "3.3.2",
    "nodemailer-smtp-transport": "2.7.4",
    "normalize.css": "8.0.1",
    "prop-types": "^15.6.2",
    "react": "16.8.3",
    "react-beautiful-dnd": "10.0.4",
    "react-dom": "16.8.3",
    "react-filepond": "7.0.1",
    "react-icons": "3.4.0",
    "react-id-swiper": "1.6.9",
    "react-textarea-autosize": "7.1.0",
    "slugify": "1.3.4"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.1.0",
    "cross-env": "5.2.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.10.0",
    "eslint": "5.14.1",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-jest": "22.3.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.1.0",
    "jest-localstorage-mock": "2.4.0",
    "nodemon": "1.18.10",
    "postcss-calc": "7.0.1",
    "postcss-preset-env": "6.5.0",
    "prettier": "1.16.4",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.8.3",
    "stylelint": "9.10.1",
    "stylelint-config-prettier": "5.0.0",
    "stylelint-prettier": "1.0.6"
  },
  "engines": {
    "node": "8.* || >=10.*"
  }
}
